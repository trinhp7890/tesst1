      {
        "type": "text",
        "id": "title_pure",
        "label": "Heading",
        "default":"This is the Rich text section"
      },
      {
        "id": "link_pure",
        "type": "url",
        "label": "Link"
      },
      {
        "id": "link_text_pure",
        "type": "text",
        "label": "Link text"
      },
      {
