	{% assign category = 'dallas-preston-royal' %}

	<div class="esthys_holder-new accordion-menu-item">
		<div class="desktop">
			<div class="custom-navigation_blogs">               
				<div class="flex-next loc_arrow-b desktop">
				   <img src="https://cdn.shopify.com/s/files/1/0386/7390/7756/files/right_arrow2-2.svg?v=1591370077"
					  alt="arrow right" width="100%">
				</div>
				<div class="flex-prev loc_arrow2-b desktop">
				   <img src="https://cdn.shopify.com/s/files/1/0386/7390/7756/files/left_arrow2-2.svg?v=1591370078"
					  alt="arrow left" width="100%">
				</div>
			  </div>
				<div class="flexslider_blogs desktop">
				   <div class="slides ">						
				{% unless blogs[category].empty? or blogs[category].articles.size == 0 %}
				{% for article in blogs[category].articles %}
				{% assign check_value = forloop.index0 | modulo: 6 %}
				{% assign check_value1 = forloop.index | modulo: 6 %}
				{% if check_value == 0 %}
				<li class="esthy_slides">
					{% endif %}
					<div class="esthy_block">
						<div class="esthy_img">
							<img class="centered-and-cropped" alt="{{ article.title }}" id="{{ article.excerpt }}"
								width="100%" style="border-radius:50%;"
								src="{{ article.image.src | img_url: '500x' }}" />
						</div>
						<div class="esthy_text">
							<div class="esthy_header">
								DALLAS PRESTON ROYAL
								<!--TO-CHANGE-->
								<div class="esthy_title centered-and-cropped">
									{{ article.title }}
								</div>
							</div>
						</div>
						<div class="esthie-hidden-content">
							{{ article.content }}
						</div>
						<div class="esthie-hidden-stuff">
							{{ article.excerpt }}
						</div>
						<input type="hidden" value="{{ article.image.src | img_url: '500x' }}"
							class="esthie-hidden-img" />
						<input type="hidden" value="{{ article.title }}" class="esthie-hidden-name" />
					</div>
					{% if check_value1 == 0 %}
				</li>
					{% endif %}

				{% if forloop.last == true %}	
				{% if forloop.index > 6 and check_value1 > 0 %}
			</li>
				{% endif %} 					
					{% if forloop.index > 0 and forloop.index <= 6 %}
						{% if forloop.index < 6%}
							</li>
						{% endif %}
							<style>
								.loc_arrow-b.desktop,
								.loc_arrow2-b.desktop {
									display: none;
								}
							</style>
					{% endif %}
				{% endif %}
		{% endfor %}
		{% endunless %}
				   </div>
	</div>
