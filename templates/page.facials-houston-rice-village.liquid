					</div>
					<div class="esthy_text">
						<div class="esthy_header">
							HOUSTON RICE VILLAGE
							<!--TO-CHANGE-->
							<div class="esthy_title centered-and-cropped">
								{{ article.title }}
							</div>
						</div>
					</div>
					<div class="esthie-hidden-content">
						{{ article.content }}
					</div>
					<div class="esthie-hidden-stuff">
						{{ article.excerpt }}
					</div>
					<input type="hidden" value="{{ article.image.src | img_url: '500x' }}" class="esthie-hidden-img" />
					<input type="hidden" value="{{ article.title }}" class="esthie-hidden-name" />
				</div>
				{% if check_value1 == 0 %}
			</li>
			{% endif %}

			{% if forloop.last == true %}
				{% if forloop.index > 2 and check_value1 > 0 %}
		</li>
				{% endif %}
				{% if forloop.index > 0 and forloop.index <= 2 %}
					</li>
					<style>
						.loc_arrow-b.mobile2,
						.loc_arrow2-b.mobile2 {
							display: none;
						}
					</style>
				{% endif %}
			{% endif %}
		{% endfor %}
	{% endunless %}
</div>
